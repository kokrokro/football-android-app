<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/editProtocolCoordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorLightGray"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:layout_gravity="top"
            android:minHeight="?attr/actionBarSize"
            app:layout_collapseMode="pin"
            app:layout_scrollFlags="scroll|enterAlways"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            android:background="@color/colorWhite">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginEnd="18sp"
                android:gravity="center_horizontal"
                android:visibility="visible">

                <ImageButton
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:scaleType="centerCrop"
                    android:visibility="invisible"
                    app:srcCompat="@drawable/ic_tick" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:fontFamily="@font/manrope_regular"
                    android:text="@string/editProtocolResponsiblePersons"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="16sp" />

                <ImageButton
                    android:id="@+id/matchResponsiblePersonsBack"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:background="@android:color/transparent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/ic_arrow_back" />
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <!--android:layout_height="wrap_content"-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8sp"
                android:orientation="vertical"
                android:paddingEnd="18dp"
                android:paddingStart="18dp"
                android:background="@color/colorWhite"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/manrope_semibold"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/inspector2"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8sp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/inspectorLogo"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/ic_logo2"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:id="@+id/inspectorName"
                        android:layout_width="180dp"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="14dp"
                        android:fontFamily="@font/manrope_regular"
                        android:paddingEnd="15sp"
                        android:paddingStart="1dp"
                        android:text="@string/empty2"
                        android:textColor="@color/colorBottomNavigationUnChecked"
                        android:textSize="14sp" />


                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8sp"
                android:orientation="vertical"
                android:paddingEnd="18dp"
                android:paddingStart="18dp"
                android:background="@color/colorWhite">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/manrope_semibold"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/referee1_2"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8sp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/referee1Logo"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/ic_logo2"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:id="@+id/referee1Name"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="14dp"
                        android:fontFamily="@font/manrope_regular"
                        android:paddingEnd="15sp"
                        android:paddingStart="1dp"
                        android:text="@string/empty2"
                        android:textColor="@color/colorBottomNavigationUnChecked"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8sp"
                android:orientation="vertical"
                android:paddingEnd="18dp"
                android:paddingStart="18dp"
                android:background="@color/colorWhite">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/manrope_semibold"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/referee2_2"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8sp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/referee2Logo"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/ic_logo2"
                        android:layout_gravity="center_vertical"/>

                    <TextView
                        android:id="@+id/referee2Name"
                        android:layout_width="180dp"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="14dp"
                        android:fontFamily="@font/manrope_regular"
                        android:paddingEnd="15sp"
                        android:paddingStart="1dp"
                        android:text="@string/empty2"
                        android:textColor="@color/colorBottomNavigationUnChecked"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8sp"
                android:orientation="vertical"
                android:paddingEnd="18dp"
                android:paddingStart="18dp"
                android:background="@color/colorWhite">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/manrope_semibold"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/referee3_2"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8sp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/referee3Logo"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/ic_logo2"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:id="@+id/referee3Name"
                        android:layout_width="180dp"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="14dp"
                        android:fontFamily="@font/manrope_regular"
                        android:paddingEnd="15sp"
                        android:paddingStart="1dp"
                        android:text="@string/empty2"
                        android:textColor="@color/colorBottomNavigationUnChecked"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:orientation="vertical"
                android:paddingEnd="18dp"
                android:paddingStart="18dp"
                android:background="@color/colorWhite">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/manrope_semibold"
                    android:paddingBottom="8dp"
                    android:paddingTop="8dp"
                    android:text="@string/referee4_2"
                    android:textColor="@color/colorBottomNavigationUnChecked"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8sp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/referee4Logo"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/ic_logo2"
                        android:layout_gravity="center_vertical" />

                    <TextView
                        android:id="@+id/referee4Name"
                        android:layout_width="180dp"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="14dp"
                        android:fontFamily="@font/manrope_regular"
                        android:paddingEnd="15sp"
                        android:paddingStart="1dp"
                        android:text="@string/empty2"
                        android:textColor="@color/colorBottomNavigationUnChecked"
                        android:textSize="14sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout><!--</LinearLayout>-->